{
  "name": "nextjs-template",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "format:check": "eslint ./src && prettier --check ./src",
    "format:fix": "eslint --fix ./src && prettier --write ./src",
    "lint": "eslint ./src",
    "prepare": "husky install",
    "pretty": "prettier --write ./src",
    "start": "yarn run start:dev",
    "start:dev": "next dev",
    "start:prod": "yarn run build && next start",
    "storybook:build": "build-storybook -c .storybook",
    "storybook:start": "start-storybook -p 6006 -c .storybook",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@svgr/webpack": "^5.5.0",
    "next": "10.2.0",
    "react": "17.0.2",
    "react-dom": "^17.0.2",
    "rsuite": "^4.10.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@storybook/addon-actions": "^6.3.2",
    "@storybook/addon-essentials": "^6.3.2",
    "@storybook/addon-links": "^6.3.2",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.3.2",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.3.0",
    "@types/react": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "autoprefixer": "^10.2.6",
    "babel-loader": "^8.2.2",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "next-compose-plugins": "^2.2.1",
    "node-sass": "^6.0.0",
    "postcss": "^8.3.5",
    "postcss-loader": "^6.1.0",
    "prettier": "2.3.0",
    "sass": "^1.35.1",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "tailwindcss": "^2.1.4",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4"
  }
}
